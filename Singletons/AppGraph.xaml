<UserControl x:Class="TransportGraphApp.Singletons.AppGraph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DockPanel
        Name="DockPanel">
            <Canvas
                Width="{Binding ActualWidth, ElementName=DockPanel}"
                Height="{Binding ActualHeight, ElementName=DockPanel}"
                Name="GraphPanel" x:FieldModifier="private">
            </Canvas>
    </DockPanel>
</UserControl>
