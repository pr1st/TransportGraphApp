<UserControl x:Class="TransportGraphApp.CustomComponents.StringRowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <WrapPanel Width="350" Height="25" Margin="5, 5, 5, 5">
        <TextBlock Width="160" FontSize="16"
                   Name="StringTitle" x:FieldModifier="private" />
        <TextBox Width="180" FontSize="16"
                 GotKeyboardFocus="ElementGotFocus"
                 Name="ValueBox" x:FieldModifier="private"/>
    </WrapPanel>
</UserControl>
